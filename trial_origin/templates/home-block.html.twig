<h1 class="center">{{ title }}</h1>
<table>
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{ items.title }}</td>
      <td>{{ items.desc }}</td>
      <td><img src=" {{ items.url }}"></td>
    </tr>
  </tbody>
</table>
